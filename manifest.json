{
  "manifest_version": 3,
  "name": "Bookmarker",
  "description": "Highlight and annotate parts of a page, as well as bookmarking pages.",
  "version": "1.0",
  "permissions": ["sidePanel", "contextMenus", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "js": ["scripts/page_content.js"],
    "css": ["css/page_content.css"],
    "matches":["<all_urls>"]
  }]
}