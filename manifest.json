{
  "manifest_version": 3,
  "name": "Hello Extensions V2",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["sidePanel", "contextMenus", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "js": ["scripts/page_content.js"],
    "matches":["<all_urls>"]
  }]
}